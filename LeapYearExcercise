
    public static void main(String[] args) {

        isLeapYear(1600);

    }


    public static boolean isLeapYear(int year){

        if(year <= 1 || year >= 9999){
            return false;
        } if(year % 4  != 0) {
            return false;
        } if(year % 100 !=0) {
            return true;
        } if(year % 400 !=0) {
            return false;
        } else {
            System.out.println("1600 is a leapyear");
            return true;
        }
    }
